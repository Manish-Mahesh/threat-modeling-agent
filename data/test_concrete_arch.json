{
  "project_name": "E-Commerce Platform",
  "description": "A simple e-commerce architecture with a web frontend and a caching layer.",
  "components": [
    {
      "name": "Nginx Web Server",
      "type": "Web Server"
    },
    {
      "name": "Redis Cache",
      "type": "Cache"
    }
  ],
  "data_flows": [
    {
      "source": "Nginx Web Server",
      "destination": "Redis Cache",
      "protocol": "RESP"
    }
  ],
  "trust_boundaries": [
    "Internet Boundary",
    "Internal Network"
  ]
}